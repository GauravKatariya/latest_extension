{
    "manifestVersion": 1.0,
    "id": "DependencyViewer",
    "version": "1.0.11",
    "name": "Dependency Viewer(Preview)",
    "description": "This tool helps track your team dependencies across feature teams.",
    "publisher": "BapTools",
    "public": false,
    "icons": {
        "default": "images/fabrikam-logo.png"
    },
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": [
        "vso.work",
        "vso.code",
        "vso.build"  
    ],
    "files": [
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "bower_components/vss-web-extension-sdk/lib/VSS.SDK.min.js",
            "addressable": true
        },
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "scripts",
            "addressable": true
        },
        {
            "path": "css",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "simple-menu-hub",
            "type": "ms.vss-web.hub",
            "description": "Adds a 'Simple' hub to the 'Sample UI' hub group to show basic usage of the menu control.",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Dependency Viewer(Preview)",
                "order": 100,
                "uri": "index.html"
            }
        }
    ]
}